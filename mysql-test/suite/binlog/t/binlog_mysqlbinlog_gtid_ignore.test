--echo #   --ignore-domain-ids and --do-domain-ids both specified
--echo # MYSQL_BINLOG MYSQLD_DATADIR/master-bin.000001 --ignore-domain-ids=0 --do-domain-ids=1 --base64-output=NEVER  > OUT_FILE
